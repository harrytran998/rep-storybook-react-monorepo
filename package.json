    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.3.0-rc.0",
    "@storybook/addons": "^6.3.0-rc.0",
    "@storybook/api": "^6.3.0-rc.0",
    "@storybook/builder-webpack5": "^6.3.0-rc.0",
